  box-shadow: 0 0 1px 0 rgba(0, 0, 0, 0.5);
}
.choose-contacts-block .select > i.toggle {
  position: absolute;
  z-index: 4;
  right: 1.5em;
  top: 1.6em;
  color: #ccc;
}
.choose-contacts-block .select .title,
.choose-contacts-block .select .placeholder {
  position: relative;
  display: block;
  width: 100%;
  height: 100%;
  padding: 1.5em 2em;
  background: white;
  border-top: 1px solid rgba(0, 0, 0, 0.05);
  cursor: pointer;
}
.choose-contacts-block .select > input {
  position: absolute;
  left: 0px;
  top: 0px;
  z-index: 1;
  width: 100%;
  height: 100%;
  display: block;
  opacity: 0;
  cursor: pointer;
	margin: 0;
}
.choose-contacts-block .select > input:checked ~ i.toggle.fa-chevron-down {
  display: none;
}
.choose-contacts-block .select > input:checked ~ i.toggle.fa-chevron-up {
  display: block;
}
.choose-contacts-block .select > input:checked div.options label.option .title {
  display: none !important;
}
.choose-contacts-block .select > input:not(:checked) {
  z-index: 4;
}
.choose-contacts-block .select > input:not(:checked) ~ label.option > span.title {
  display: none;
}
.choose-contacts-block .select > input:not(:checked) ~ i.toggle.fa-chevron-up {
  display: none;
}
.choose-contacts-block .select > input:not(:checked) ~ i.toggle.fa-chevron-down {
  display: block;
}
.choose-contacts-block .select > input:disabled {
  cursor: no-drop;
}
.choose-contacts-block .select > span.placeholder {
  position: relative;
  z-index: 0;
  display: inline-block;
  width: 100%;
  color: #303133;
	font-weight: 600;
  border-top: 0px;
}
.choose-contacts-block .select label.option {
  display: block;
  overflow: hidden;
  z-index: 1;
  width: 100%;
  transition: all 1s ease-out;
	margin: 0;
	color: #303133;
	font-weight: 600;
}
.choose-contacts-block .select label.option span.title {
  position: relative;
  z-index: 2;
  transition: background .3s ease-out;
}
.choose-contacts-block .select label.option span.title i.icon {
  padding-right: 8px;
  color: #92a8d1;
}
.choose-contacts-block .select label.option span.title:hover {
  color: #fff;
  background: #3ECF8E;
  box-shadow: inset 0px 1px 0px rgba(0, 0, 0, 0.1);
}
.choose-contacts-block .select label.option input {
  display: none;
}
.choose-contacts-block .select label.option input:checked ~ span.title {
  position: absolute;
  display: block;
  z-index: 3;
  top: 0px;
  font-size: 14px;
  background: #fff;
  border-top: 0px;
  box-shadow: none;
  color: inherit;
  width: 100%;
}
.choose-contacts-block .select label.option input:disabled ~ span.title {
  background: #f9f9f9 !important;
  color: #aaa;
}
.choose-contacts-block .select label.option input:disabled ~ span.title:hover {
  color: #aaa;
  background: none;
  cursor: no-drop;
}
